# example environment variables for local development or production build
# default port for CRA dev server
PORT=3005

# URL used by StreamList to fetch the list of streams
REACT_APP_API_URL=http://localhost:3000

# WebSocket address used by StreamViewer
REACT_APP_SIGNALING_SERVER=ws://localhost:3000

# optional TURN server URL used by viewers, e.g. turn:myhost:3478
REACT_APP_TURN_URL=turn:localhost:3478
